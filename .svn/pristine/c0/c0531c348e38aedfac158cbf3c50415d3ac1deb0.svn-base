package mainMVC.model.dataPacket;

import java.io.Serializable;
/**
 * class used for communication between client and game server
 * itself is a data of String
 * @author Bojun
 *
 */
public class MyStringMsg implements Serializable{
	private static final long serialVersionUID = 4755063879390205778L;
	private String _string;
	/**
	 * constructor
	 * @param s
	 */
	public MyStringMsg(String s){
		_string = s;
	}
	/**
	 * getter
	 * @return
	 */
	public String getString(){
		return _string;
	}
}
