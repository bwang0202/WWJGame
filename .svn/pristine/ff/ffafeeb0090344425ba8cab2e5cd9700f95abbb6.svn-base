package mainMVC.view;
/**
 * interface between miniview and mini model
 * @author Bojun
 *
 */
public interface IMiniView2ModelAdapter {
	/**
	 * send a text message to mini model
	 * @param text
	 */
	public abstract void sendTextMessage(String text);
	/**
	 * tells the model that i am leaving the chatroom
	 * then the model should sent a IRemoveUser to chatroom
	 */
	public abstract void leave();

}
