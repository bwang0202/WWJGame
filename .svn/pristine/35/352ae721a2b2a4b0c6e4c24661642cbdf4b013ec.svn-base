package mainMVC.model.dataPacket;

import java.io.Serializable;
/**
 * a game status message containing a string that will describe game status in a proper manner
 * @author Bojun
 *
 */
public class MyGameStatusMsg implements Serializable{
	private static final long serialVersionUID = 357219488500436441L;
	private String _string;
	/**
	 * constructor 
	 * @param s the string representing game status
	 */
	public MyGameStatusMsg(String s){
		_string = s;
	}
	/**
	 * getter for the string
	 * @return
	 */
	public String getString(){
		return _string;
	}
}