package mainMVC.model.dataPacket;

import java.awt.Component;
import java.io.Serializable;

/**
 * a class containing a java awt Component
 * usually containing an empty jframe, and set up all other components in the cmd that process this frame.
 * @author Bojun
 *
 */
public class MyMapFrameMsg implements Serializable{
	/**
	 * generated uid
	 */
	private static final long serialVersionUID = -4083068731979284490L;
	private Component _comp;
	/**
	 * constructor, setter
	 * @param comp
	 */
	public MyMapFrameMsg(Component comp){
		_comp = comp;
	}
	/**
	 * getter of the component
	 * @return
	 */
	public Component getMapFrame(){
		return _comp;
	}

}
